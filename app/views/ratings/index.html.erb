<% provide(:title, 'All ratings') %>
<h1>Ratings for: <%= @course.name%> </h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">

  <ul id="check-mark">
    <% @ratings.each do |f| %>
      <li>Rating by <%= f.student.first_name %>: <%= f.rating %></li>
    <% end %>
  </ul>
 </div>
</div>
<p id="text-center"><%= link_to 'Back', ratings_path %></p>
