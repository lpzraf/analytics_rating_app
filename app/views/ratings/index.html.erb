<% provide(:title, 'All ratings') %>
<h1>Ratings for <%= @course.name%> </h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">

  <ul class"students">
    <%= @ratings.each do |f| %>
      <li>Rating by <%= f.student.first_name %>: <%= f.rating %></li>
    <% end %>
  </ul>

 </div>
</div>
