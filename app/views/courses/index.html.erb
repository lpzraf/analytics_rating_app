<h1>Courses</h1>
<%= link_to 'New course', new_course_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Instructor</th>
    <th></th>
  </tr>

  <% @courses.each do |course| %>
    <tr>
      <td><%= course.name %></td>
      <td><%= course.instructor %></td>
      <td><%= link_to 'Show', course_path(course) %></td>
      <td><%= link_to 'Edit', edit_course_path(course) %></td>
    </tr>
  <% end %>
</table>
